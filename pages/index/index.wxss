/* 搜索样式 */
.search {
  padding: 32rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-around;
  overflow: hidden;
}

.b_center_search {
  flex: 1;
  height: 56rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 6px 17px -3px rgba(41, 41, 157, 0.07);
  border-radius: 10px;
  display: flex;
  align-items: center
}

.p_search_img {
  width: 32rpx;
  height: 32rpx;
  flex: 0 0 32rpx;
  margin: 0 26rpx;
}

.p_search_input {
  font-size: 28rpx;
  color: #070731;
}


.scroll_box {
  width: 100%;
  overflow: hidden;
  background: #fff;
  white-space: nowrap;
}
.cate{
  padding: 0rpx 2rpx;
  text-align: center;
  box-sizing: border-box;
  border-radius:10px;
  height: 56rpx;
  line-height: 56rpx;
  flex: 0;
  white-space: nowrap;
  margin-right: 24rpx;
  color:#0F1E3E;
  font-size: 28rpx;
  position: relative;
}
.cate-active{
  font-weight: bold;
}
.cate-active::after{
  display: inline-block;
  content: '';
  bottom: 10rpx;
  z-index: -1;
  left: 0rpx;
  right: 0rpx;
  height: 16rpx;
  background:rgba(74,71,255,0.5);
  position: absolute;
  
}
.icon-msg{
  margin-left: 16rpx;
  flex: 0 0 64rpx;
  width: 64rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
  box-sizing: border-box;
  background:rgba(242,242,255,1);
border-radius:4px;
}
.icon-msg image{
  width: 32rpx;
  height: 32rpx;
}
.main-tit{
  display: flex;
  align-items: center;
